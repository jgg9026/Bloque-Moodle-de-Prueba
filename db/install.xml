    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_simplehtml" COMMENT="Default comment for block_simplehtml, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="blockid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Block id"/>
        <FIELD NAME="pagetitle" TYPE="char" LENGTH="30" NOTNULL="true" SEQUENCE="false" COMMENT="Link Title"/>
        <FIELD NAME="linkdescription" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="A description for your link"/>
        <FIELD NAME="linkurl" TYPE="char" LENGTH="1333" NOTNULL="true" SEQUENCE="false" COMMENT="Url"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
